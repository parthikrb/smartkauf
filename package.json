{
  "name": "smartkauf",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "format": "prettier --write ./src",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx,.ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "expo": "44.0.6",
    "expo-status-bar": "1.2.0",
    "jest": "^27.1.3",
    "jest-expo": "^44.0.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-web": "0.17.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.7",
    "@types/jest": "^27.4.1",
    "@types/react": "17.0.41",
    "@types/react-native": "0.64.24",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "cypress": "^9.5.2",
    "eslint": "8.11.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "7.0.4",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "12.3.7",
    "prettier": "2.6.0",
    "react-test-renderer": "17",
    "ts-jest": "^27.1.3",
    "typescript": "4.6.2"
  },
  "resolutions": {
    "react-native/@jest/create-cache-key-function": "^27.0.2"
  },
  "private": true
}
