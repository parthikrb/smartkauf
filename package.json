{
  "name": "smartkauf",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__",
    "lint": "eslint --ext .js,.ts,.tsx src/",
    "lint:fix": "eslint --ext .js,.ts,.tsx src/ --fix",
    "format": "prettier --write ./src",
    "prepare": "husky install",
    "generate": "graphql-codegen -r dotenv/config --config codegen.yml"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --ext .tsx,.ts src/ --fix"
    ],
    "./src/**": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@apollo/client": "3.6.2",
    "@expo/vector-icons": "13.0.0",
    "@react-native-segmented-control/segmented-control": "2.4.0",
    "@react-navigation/bottom-tabs": "6.3.1",
    "@react-navigation/native": "6.0.10",
    "@react-navigation/native-stack": "6.6.2",
    "@sentry/react-native": "^3.4.1",
    "algoliasearch": "4.13.0",
    "cross-fetch": "3.1.5",
    "dotenv": "16.0.0",
    "expo": "44.0.6",
    "expo-application": "~4.0.1",
    "expo-blur": "11.0.0",
    "expo-constants": "~13.0.1",
    "expo-device": "~4.1.0",
    "expo-location": "14.0.2",
    "expo-status-bar": "1.2.0",
    "jest": "28.0.3",
    "expo-updates": "~0.11.6",
    "graphql": "16.4.0",
    "jest-expo": "44.0.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-instantsearch-native": "6.24.0",
    "react-native": "0.68.1",
    "react-native-btr": "2.1.3",
    "react-native-dotenv": "3.3.1",
    "react-native-safe-area-context": "4.2.5",
    "react-native-web": "0.17.7",
    "sentry-expo": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@graphql-codegen/add": "3.1.1",
    "@graphql-codegen/cli": "2.6.2",
    "@graphql-codegen/fragment-matcher": "3.2.1",
    "@graphql-codegen/introspection": "2.1.1",
    "@graphql-codegen/typescript": "2.4.8",
    "@graphql-codegen/typescript-document-nodes": "2.2.8",
    "@graphql-codegen/typescript-graphql-files-modules": "2.1.1",
    "@graphql-codegen/typescript-operations": "2.3.5",
    "@graphql-codegen/typescript-react-apollo": "3.2.11",
    "@react-native/assets": "1.0.0",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/react-native": "9.1.0",
    "@types/jest": "27.5.0",
    "@types/react": "18.0.1",
    "@types/react-instantsearch-native": "6.3.2",
    "@types/react-native": "0.67.6",
    "@types/react-test-renderer": "18.0.0",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "cypress": "9.6.0",
    "eslint": "8.14.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-unicorn": "42.0.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest-coverage-badges": "1.1.2",
    "jest-transform-stub": "2.0.0",
    "lint-staged": "12.4.1",
    "prettier": "2.6.2",
    "react-native-gesture-handler": "2.4.1",
    "react-native-reanimated": "2.8.0",
    "react-native-screens": "3.13.1",
    "react-test-renderer": "18.1.0",
    "ts-jest": "27.1.4",
    "typescript": "4.6.4"
  },
  "resolutions": {
    "react-native/@jest/create-cache-key-function": "^28.0.0"
  },
  "private": true
}
